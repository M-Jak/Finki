-- treta zadaca
-- bi trebalo da e tocna, mislam deka greskata e na strana na finki a ne moja
select *
from Grad g join Sosedi s on g.id_mesto=s.grad1 or g.id_mesto=s.grad2
join Objekt o ON g.id_mesto=o.id_grad JOIN Poseta p ON o.id_mesto=p.id_mesto JOIN Korisnik k ON k.kor_ime=p.kor_ime
where (g.id_mesto=s.grad1 or g.id_mesto=s.grad2) and s.rastojanie<300;